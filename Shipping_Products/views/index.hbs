{{#extends "base"}}
{{#block "content"}}
<h1>Shipping Products</h1>
<table class="table">
    <tr>
        <th>Product Id</th>
        <th>Product Name</th>
        <th>Price</th>
        <th>Product Type</th>
        <th>Shipping Company</th>
    </tr>
    {{#each shippingProducts}}
    <tr>
        <td>{{ProductsId}}</td>
        <td>{{name}}</td>
        <td>{{price}}</td>
        <td>{{typeName}}</td>
        <td>{{companyName}}</td>
    </tr>
    {{/each}}
</table>
{{/block}}
{{/extends}}


